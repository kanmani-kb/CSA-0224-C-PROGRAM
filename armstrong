#include<stdio.h>
int main()
{
    int num,r=0,digit,p;
    scanf("%d",&num);
    p=num;
    while(num!=0){
        digit=num%10;
        r=r+(digit*digit*digit);
        num=num/10;
    }
   if(p==r){
       printf("it is an armstrong num",r);
   }
   else{
       printf("it is not an armstrong num");
   }
    return 0;
}
