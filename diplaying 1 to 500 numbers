#include <stdio.h>

int main() {
    int i, num, sum, temp;

    printf("Magic numbers between 1 and 500:\n");

    for (i = 1; i <= 500; i++) {
        num = i;

        while (num > 9) {
            sum = 0;
            temp = num;

            while (temp > 0) {
                sum += temp % 10;
                temp /= 10;
            }

            num = sum;
        }

        if (num == 1)
            printf("%d ", i);
    }

    return 0;
}
