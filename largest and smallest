#include<stdio.h>

    int main() {
    int a, b, c;
    int largest, smallest;

    scanf("%d %d %d", &a, &b, &c);

    
    if (a > b)
        largest = a;
    else
        largest = b;

    if (c > largest)
        largest = c;

   
    if (a < b)
        smallest = a;
    else
        smallest = b;

    if (c < smallest)
        smallest = c;

    printf("Largest = %d\n", largest);
    printf("Smallest = %d\n", smallest);

    return 0;
}
