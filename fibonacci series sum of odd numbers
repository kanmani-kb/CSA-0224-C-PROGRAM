#include<stdio.h>
int main()
{
    int n,i,next,first=0,second=1,odd,sum;
    scanf("%d",&n);
  
    for(i=0;i<n;i++){
        printf("%d\n",first);
        if(first %2!=0)
        sum=sum+first;
        
        next=first+second;
        first=second;
        second=next;
    }
    
    printf("the sum of odd numbers=%d",sum);
    return 0;
}
