#include <stdio.h>

int main() {
    int arr[5] = {10, 20, 30, 40, 50};
    int index, newValue;

    printf("Enter index (0 to 4): ");
    scanf("%d", &index);

    if (index < 0 || index >= 5) {
        printf("Invalid index!\n");
        return 1;
    }

    printf("Enter new value: ");
    scanf("%d", &newValue);

    arr[index] = newValue;

    printf("Updated array:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
