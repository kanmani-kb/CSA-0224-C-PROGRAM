#include <stdio.h>

int main() {
    int n;
    long long product = 1;   // use long long to avoid overflow for big values

    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        product *= arr[i];
    }

    printf("Product of array elements = %lld\n", product);

    return 0;
}
