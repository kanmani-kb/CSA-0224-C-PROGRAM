#include <stdio.h>

int main() {
    int n;
    int sum = 0;

    printf("Enter total number of elements (including missing one): ");
    scanf("%d", &n);

    int arr[n - 1];

    printf("Enter %d elements:\n", n - 1);
    for (int i = 0; i < n - 1; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    int expectedSum = n * (n + 1) / 2;
    int missing = expectedSum - sum;

    printf("Missing element = %d\n", missing);

    return 0;
}
